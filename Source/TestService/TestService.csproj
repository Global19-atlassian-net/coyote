<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Description>The Coyote systematic testing service.</Description>
    <AssemblyName>Microsoft.Coyote.TestService</AssemblyName>
    <RootNamespace>Microsoft.Coyote.TestService</RootNamespace>
    <PackageTags>systematic-testing;scheduling;service</PackageTags>
    <OutputPath>..\..\bin\</OutputPath>
    <CoyoteTargetType>Application</CoyoteTargetType>
    <Framework48Supported>false</Framework48Supported>
    <NetCore31Supported>false</NetCore31Supported>
  </PropertyGroup>
  <Import Project="..\..\Common\build.props" />
  <Import Project="..\..\Common\key.props" />
  <ItemGroup>
    <Protobuf Include="Interfaces\coyote.proto" GrpcServices="Server" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Grpc.AspNetCore" Version="2.32.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Source\Core\Core.csproj" />
  </ItemGroup>
</Project>
